pipeline {
   agent any
   
   environment {
       DEMO='1.3'
   }

   stages {
      stage('stage-1') {
         steps {
            echo "This is build number $BUILD_NUMBER of demo $DEMO"
            sh '''
               ls -l
               echo "Using a multi-line shell step"
               chmod +x pipelines/test.sh
               ./pipelines/test.sh
            '''
         }
      }
   }
}