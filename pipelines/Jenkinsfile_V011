pipeline {
   agent {
      docker {
         image 'mcr.microsoft.com/dotnet/core/sdk:3.1.101'
      }
   }
   stages {
      stage('Verify') {
         steps {
               sh '''
               dotnet --list-sdks
               dotnet --list-runtimes
               '''
               sh 'printenv'
               sh 'ls -l "$WORKSPACE"'
         }
      }
   }
}